{
  "name": "@mbykov/anthrax",
  "version": "1.1.1",
  "description": "simple morphological analyzer of the ancient Greek language",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node run.js",
    "names_": "cross-env NODE_ENV=test mocha -g $FILTER -c -R dot -b --timeout 5000 'test/wkt_name.test.js' --check-leaks --no-deprecation",
    "name": "mocha -c -R dot -b test/wkt_name.test.js --check-leaks --timeout 5000 ",
    "verb": "mocha -c -R dot -b test/wkt_verb.test.js --check-leaks --timeout 5000 ",
    "names_jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/wkt_name.test.js  --noStackTrace --bail",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js test/wkt_name.test.js  --noStackTrace --bail",
    "simple": "mocha -c -R dot -b --full-trace true test/simple_test.js",
    "pref": "mocha -c -R dot -b --full-trace true test/prefix_test.js",
    "test_": "jest"
  },
  "jest": {
    "verbose": true,
    "bail": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "reporters": [
      "jest-progress-bar-reporter"
    ]
  },
  "author": "mbykov <m.bykov@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "fs-extra": "^10.0.0",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "node-fetch": "^3.2.0",
    "orthos": "^0.7.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "debug": "^4.3.2",
    "jest": "^27.4.3",
    "jest-progress-bar-reporter": "^1.0.21",
    "mocha": "^9.1.3"
  }
}
